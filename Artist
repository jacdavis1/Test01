<?php
class Artist {
    public function __construct($ArtID, $ArtName, $ArtMana, $Art) {
        $this->ArtID = $ArtID;
        $this->ArtName = $ArtName;
        $this->ArtMana = $ArtMana;
        $this->ArtCont = $ArtCont;
    }

    public function quick_details() {
        echo "This is " . $this->ArtName . " who is managed by " . $this->ArtMana . ".\n";
    }
}

$art0001 = new Artist(0001, "", "", "");
$art0001->quick_details();
